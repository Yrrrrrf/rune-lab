{
  "name": "@yrrrrrf/rune-lab",
  "version": "0.0.6",
  "description": "Rune Lab is your modern toolkit for crafting stunning, reactive web applications with Svelte 5.",
  "license": "MIT",
  "exports": "./src/lib/mod.ts",
  "tasks": {
    "dev": "vite dev",
    "build": "vite build && deno task prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint"
  },
  "imports": {
    "$lib/": "./src/lib/",
    "$lib/paraglide/": "./src/lib/paraglide/",
    "svelte": "npm:svelte@^5.50.0",
    "daisyui": "npm:daisyui@^5.5.18",
    "tailwindcss": "npm:tailwindcss@^4.1.18",
    "@inlang/paraglide-js": "npm:@inlang/paraglide-js@^2.9.0",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.18",
    "@tailwindcss/forms": "npm:@tailwindcss/forms@^0.5.11",
    "@tailwindcss/typography": "npm:@tailwindcss/typography@^0.5.19",
    "vite": "npm:vite@^7.3.1",
    "typescript": "npm:typescript@^5.9.3",
    "publint": "npm:publint@^0.3.2",
    "svelte-check": "npm:svelte-check@^4.3.6",
    "@sveltejs/kit": "npm:@sveltejs/kit@^2.50.2",
    "@sveltejs/adapter-static": "npm:@sveltejs/adapter-static@^3.0.10",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^6.2.4",
    "@sveltejs/package": "npm:@sveltejs/package@^2.5.7"
  },
  "nodeModulesDir": "auto",
  "publish": {
    "include": [
      "src/lib",
      "README.md",
      "LICENSE",
      "messages"
    ],
    "exclude": [
      ".github/",
      ".svelte-kit/",
      ".vite/",
      "dist/",
      "node_modules/"
    ]
  },
  "jsr": {
    "peerDependencies": {
      "svelte": "^5.50.2"
    }
  }
}
